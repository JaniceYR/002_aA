
<html>
  <head>
    <title>MusicApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
      <%= current_user.email %>
      <%= button_to "Sign Out", session_url, method: :delete%>
    <% end %>
    <%= yield %>
  </body>
</html>
